{
    "extends": ["airbnb-base", "prettier", "plugin:node/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "import/extensions": "off",
        "node/no-unpublished-import": "off",
        "no-console": "off",
        "no-process-exit": "off"
    }
}